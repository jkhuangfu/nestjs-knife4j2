  "license": "MIT",
  "keywords": [
    "nestjs",
    "knife4j",
    "swagger",
    "openapi",
    "api",
    "doc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkhuangfu/nestjs-knife4j2.git"
  },
  "publishConfig": {
